\" class=\"create-twoot-panel\" @submit.prevent=\"createNewTwoot\" :class=\"{'--exceeded': newTwootCharacterCount > 180}\">\n          <label for=\"newTwoot\"> <strong>New Twoot</strong>({{ newTwootCharacterCount }}/180)</label> \n          <textarea name=\"\" id=\"newTwoot\" rows=\"4\" v-model=\"state.newTwootContent\"/>\n\n          <div class=\"create-twoot-panel__submit\">\n              <div class=\"create-twoot-type\">\n            <label for=\"newTwootType\">\n              <strong>Type:</strong>\n            </label>\n            <select id=\"newTwootType\" v-model=\"state.selectedTwootType\">\n              <option :value=\"option.value\" v-for=\"(option, index) in state.twootTypes\" :key=\"index\" >\n                {{ option.name }}\n              </option>\n            </select>\n          </div>\n          <button class=\"btn\">\n            Twoot it!\n          </button>\n          </div>\n        </form>\n</template>\n\n<script>\nimport { reactive, computed } from 'vue';\n\nexport default {\n    name: \"CreateTwootPanel\",\n\n    setup(props, ctx) {\n        const state = reactive({\n            newTwootContent: '',\n      selectedTwootType: 'instant',\n      twootTypes: [\n        { value: 'draft', name: 'Draft' },\n        { value: 'instant', name: 'Instant Twoot'}\n        ],\n    })\n\n    const newTwootCharacterCount = computed(() => state.newTwootContent.length) \n\n    function createNewTwoot() {\n    if (state.newTwootContent && state.selectedTwootType !== 'draft') {\n      ctx.emit('add-twoot', state.newTwootcontent) \n      state.newTwootContent = '';\n    }\n    }\n\n        return {\n            state,\n            newTwootCharacterCount,\n            createNewTwoot\n\n        }\n   \n    },\n\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.create-twoot-panel {\n  margin-top: 20px;\n  padding: 20px 0;\n  display: flex;\n  flex-direction: column;\n  textarea {\n    border: 1px solid #DFE3E8;\n    border-radius: 5px;\n  }\n  .create-twoot-panel__submit {\n    display: flex;\n    justify-content: space-between;\n    .create-twoot-type {\n      padding: 10px 0;\n    }\n    button {\n      padding: 5px 20px;\n      margin: auto 0;\n      border-radius: 5px;\n      border: none;\n      background-color: deeppink;\n      color: white;\n      font-weight: bold;\n     \n    }\n  }\n  &.--exceeded {\n    color: red;\n    border-color: red;\n    .create-twoot-panel__submit {\n      button {\n        background-color: red;\n        color: white;\n      }\n    }\n  }\n}\n</style>"],"sourceRoot":""}]}